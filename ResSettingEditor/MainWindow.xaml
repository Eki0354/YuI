<Window x:Class="ResSettingEditor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ResSettingEditor"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="600" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <TreeView x:Name="treeView" HorizontalAlignment="Left" Height="414" Margin="0,10,0,0" VerticalAlignment="Top" Width="135"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="414" Margin="135,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="459"/>

    </Grid>
</Window>
